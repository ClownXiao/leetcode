public class Solution {
    public void nextPermutation(int[] nums) {
        int target = 0;
        for(int i = nums.length-1; i > 0; i--){
            if(nums[i] > nums[i-1]){
                int x = i+1;
                while( x < nums.length&& nums[x] > nums[i-1]){
                    x++;
                }
                x--;
                int temp = nums[x];
                nums[x] = nums[i-1];
                nums[i-1] = temp;
                target = i;
                break;
            }
        }
        int l = target;
        int h = nums.length - 1;
        while(l < h){
            int temp = nums[l];
                nums[l] = nums[h];
                nums[h] = temp;
                l++;
                h--;
        }
    }
}
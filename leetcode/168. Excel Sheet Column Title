public class Solution {
    public String convertToTitle(int n) {
            n--;
            int offset = 65;
            int x = n / 26;
            int y = n % 26;
            if(x >= 1)
                return convertToTitle(x) + (char)(y + offset);
            else{
                return "" + (char)(y + offset);
            }
    }
    
    
}
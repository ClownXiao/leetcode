public class Solution {
    public boolean isValid(String s) {
        List l = new ArrayList();
	       char temp;
	       for(int i = 0; i < s.length(); i++){
	    	   temp = s.charAt(i);
	    	   switch(temp){
	    	   case '(': l.add(')');
	    	   			break;
	    	   case '{': l.add('}');
	   					break;
	    	   case '[': l.add(']');
	   					break;
	    	   case ']':
	    	   case ')':
	    	   case '}':
	    	            if(l.size() == 0) return false;
	    		   		if(l.get(l.size()-1).equals(temp)){
	    		   			l.remove(l.size()-1);
	    		   			break;
	    		   		}
	    		   		else return false;
	    	   default: return false;
	    	   }
	       }

	    	return l.isEmpty();
    }
}